{
  "expo": {
    "name": "zet_design",
    "slug": "zet_design",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.zet-design"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.zet_design"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      "expo-asset"
    ],
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*",
      "src/assets/**/*",
      "src/assets/rive/*.riv"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      },
      "enableRiveReactNative": true
    }
  }
}
